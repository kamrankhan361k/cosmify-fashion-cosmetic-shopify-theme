.page-width {
  will-change: filter, transform, opacity;
  -webkit-backface-visibility: hidden;
  animation-fill-mode: forwards;
  opacity: 0 !important;
  transition: filter 1.3s, transform 1.6s, opacity 0.7s ease-in;
}
