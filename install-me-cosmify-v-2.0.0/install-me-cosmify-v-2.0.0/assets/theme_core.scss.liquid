  margin: 0 2rem;
  top: 50%;z-index:3;
  -ms-transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}
.main-brand {
  display: flex;
  .heading-block {
    h3 {
      font-size: 30px;
      margin: 0;
      font-weight: 700;
      color: #fff;
      &:after {
        content: "";
        background: #fff;
        height: 2px;
        width: 50px;
        position: absolute;
        left: 50%;
        bottom: -15px;
        transform: translateX(-50%);
      }
    }
  }
  #logo-brand {
    img {
      width: auto !important;
    }
    &.type-1 {
      .banner-items {
        padding: 0 10px;
        position: relative;
        .content-banner {
          position: absolute;
          text-align: center;
          left: 50%;
          bottom: 25px;
          transform: translateX(-50%);
          -webkit-transform: translateX(-50%);
          -moz-transform: translateX(-50%);
          width: 100%;
          a {
            height: 45px;
            line-height: 45px;
            display: inline-block;
            padding: 0 35px;
            font-size: 14px;
            font-weight: initial !important;
            letter-spacing: 0;
            margin-top: 0px;
            @extend .clv-btn-1;
          }
        }
      }
    }
    &.type-2 {
      .banner-items {
        padding: 0 15px;
        position: relative;
        .content-banner {
          display: none;
        }
      }
    }
    &.type-3 {
      .banner-items {
        position: relative;
        .content-banner {
          position: absolute;
          text-align: left;
          left: 50%;
          bottom: 30px;
          transform: translateX(-50%);
          -webkit-transform: translateX(-50%);
          -moz-transform: translateX(-50%);
          width: 80%;
          background: #ffffff;
          padding: 15px 35px;
          @media (max-width: 767px) {
            padding: 10px;
          }
          > div {
            text-transform: capitalize;
            font-size: 40px;
            letter-spacing: 0;
            font-weight: 400;
            color: #000;
            line-height: 50px;
            @media (max-width: 767px) {
              font-size: 24px;
              line-height: 30px;
            }
          }
          a {
            line-height: 30px;
            font-size: 14px;
            font-weight: 400;
            letter-spacing: 0;
            color: #999999;
          }
        }
      }
    }
    &.type-4 {
      .banner-items {
        padding: 0 15px;
        position: relative;
        .content-banner {
          h3 {
            font-size: 18px;
            line-height: 24px;
            font-weight: 600;
            color: #000;
            padding: 25px 0 10px;
            margin: 0;
          }
          p {
            font-size: 14px;
            line-height: 28px;
            font-weight: 400;
            color: #828282;
            margin: 0;
          }
          a {
            height: 45px;
            line-height: 45px;
            display: inline-block;
            padding: 0 35px;
            font-size: 14px;
            font-weight: initial !important;
            letter-spacing: 0;
            margin-top: 30px;
            @extend .clv-btn-1;
          }
        }
      }
    }
  }
}
.block-heading {
  .heading-title {
    p {
      font-size: 16px;
      text-transform: unset;
      color: #7d7d7d;
      line-height: 30px;
      margin-top: 7px;
      margin-bottom: 0;
      font-weight: 400;
    }
  }
}

.home-carousel-banner {
  .slick-slide {
    transform: scale(0.78);
    transition: all 0.3s ease 0s;
    -moz-transition: all 0.3s ease 0s;
    -webkit-transition: all 0.3s ease 0s;
    -o-transition: all 0.3s ease 0s;
    &.slick-active {
      transform: scale(1);
      transition: all 0.3s ease 0s;
      -moz-transition: all 0.3s ease 0s;
      -webkit-transition: all 0.3s ease 0s;
      -o-transition: all 0.3s ease 0s;
      &.home-collection-item {
        .home-collection-title-wrapper {
          opacity: 1;
        }
      }
    }
    &.home-collection-item {
      position: relative;
      overflow: hidden;
      padding: 0;
      > a {
        position: relative;
        display: inline-block;
        width: 100%;
      }
      .home-collection-title-wrapper {
        position: absolute;
        bottom: 10% !important;
        text-align: center;
        width: 100%;
        z-index: 9;
        height: auto !important;
        padding: 0 !important;
        background: transparent !important;
        left: auto !important;
        top: auto !important;
        right: auto !important;
        opacity: 0;
        .home-collection-title {
          h3 {
            a {
              text-transform: capitalize;
              text-decoration: none;
              font-size: 34px;
              line-height: 40px;
              letter-spacing: 0;
              white-space: normal;
            }
          }
          p {
            font-size: 20px;
            line-height: 25px;
            padding-top: 10px;
            color: #dc2f47;
          }
          .link-home4 {
            a {
              height: 45px;
              line-height: 45px;
              display: inline-block;
              padding: 0 35px;
              font-size: 14px;
              font-weight: initial !important;
              letter-spacing: 0;
              margin-top: 0px;
              @extend .clv-btn-1;
            }
          }
        }
      }
    }
  }
}
