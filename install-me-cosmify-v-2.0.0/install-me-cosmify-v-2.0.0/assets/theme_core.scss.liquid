/* Settings color  */

/* Body */
$body-text: {{ settings.body_color }};
$body-link: {{ settings.body_link_color }};
$body-hover: {{ settings.body_link_color_hover }};
$body-bg: {{ settings.bg_body_color }};
$body-font-size: {{ settings.body_font_size }};

/* HEADER Promotion */
$header-promotion-bg: {{ settings.promotion_box_bg_color }};
$header-promotion-text: {{ settings.promotion_box_color }};

/* HEADER Menu Top */
$header-top-bg: {{ settings.bg_header_top_color }};
$header-top-link: {{ settings.header_top_link_color }};
$header-top-link-hover: {{ settings.header_top_link_color_hover }};
$header-top-text: {{ settings.header_top_text_color }};

/* HEADER Menu Bottom */
$header-bottom-bg: {{ settings.bg_header_bottom_color }};
$header-bottom-link: {{ settings.header_bottom_link_color }};
$header-bottom-link-hover: {{ settings.header_bottom_link_color_hover }};
$header-bottom-text: {{ settings.header_bottom_text_color }};

/* HEADER Menu */
$header-bg: {{ settings.bg_header_color }};
$header-link: {{ settings.link_color_levertop }};
$header-link-hover: {{ settings.link_color_levertop_hover }};
$header-bg-dropdown: {{ settings.background_dropdown }};
$header-dr-title-text: {{ settings.link_color_title }};
$header-dr-link: {{ settings.link_color_sub_menu }};
$header-dr-link-hover: {{ settings.link_color_sub_menu_hover }};

/* HEADER Sticky Menu */
$header-sticky-bg: {{ settings.background_megamenu_ticky }};
$header-sticky-link: {{ settings.link_megamenu_ticky }};
$header-sticky-hover: {{ settings.link_hover_megamenu_ticky }};
$header-text-count: {{ settings.color_megamenu_ticky_text_cart }};
$header-bg-count: {{ settings.color_megamenu_ticky_icon_cart }};

/* Color Slider */
$white-slider-headding-1: {{ settings.white_slider_headding_1 }};
$white-slider-headding-2: {{ settings.white_slider_headding_2 }};
$white-slider-headding-3: {{ settings.white_slider_headding_3 }};

$dark-slider-headding-1: {{ settings.dark_slider_headding_1 }};
$dark-slider-headding-2: {{ settings.dark_slider_headding_2 }};
$dark-slider-headding-3: {{ settings.dark_slider_headding_3 }};

/* Footer */
$footer-bg: {{ settings.bg_footer_color }};
$footer-title: {{ settings.footer_title_color }};
$footer-text: {{ settings.footer_text_color }};
$footer-link: {{ settings.footer_link_color }};
$footer-hover: {{ settings.footer_link_hover }};

/* Product Label Color */
$label-sale-text: {{ settings.sale_tag_color }};
$label-sale-border: {{ settings.sale_tag_bordercolor }};
$label-sale-bg: {{ settings.Second_sale_tag_bg_color }};
$label-new-text: {{ settings.new_tag_color }};
$label-new-border: {{ settings.new_tag_bordercolor }};
$label-new-bg: {{ settings.Second_new_tag_bg_color }};

/* Price Color */
$price-compare: {{ settings.price_color_compare }};
$price: {{ settings.price_color }};

/* Breadcrumb */
$breadcrumb-bg: {{ settings.breadcrumb_bg_color }};
$breadcrumb-border: {{ settings.breadcrumb_border_color }};
$breadcrumb-text: {{ settings.breadcrumb_color }};

/* BUTTON TYPE 1 */
$primary-bt-bg: {{ settings.primary_button_bg_color }};
$primary-bt-border: {{ settings.primary_button_border_color }};
$primary-bt-text: {{ settings.primary_button_text_color }};
$primary-bt-bg-hover: {{ settings.primary_button_bg_color_hover }};
$primary-bt-border-hover: {{ settings.primary_button_border_color_hover }};
$primary-bt-text-hover: {{ settings.primary_button_text_color_hover }};

/* BUTTON TYPE 2 */
$secon-bt-bg: {{ settings.second_button_bg_color }};
$secon-bt-border: {{ settings.second_button_border_color }};
$secon-bt-text: {{ settings.second_button_text_color }};
$secon-bt-bg-hover: {{ settings.second_button_bg_color_hover }};
$secon-bt-border-hover: {{ settings.second_button_border_color_hover }};
$secon-bt-text-hover: {{ settings.second_button_text_color_hover }};

/* BUTTON TYPE 3 */
$secon-bt-bg_3: {{ settings.second_button_bg_color_3 }};
$secon-bt-border_3: {{ settings.second_button_border_color_3 }};
$secon-bt-text_3: {{ settings.second_button_text_color_3 }};
$secon-bt-bg-hover_3: {{ settings.second_button_bg_color_hover_3 }};
$secon-bt-border-hover_3: {{ settings.second_button_border_color_hover_3 }};
$secon-bt-text-hover_3: {{ settings.second_button_text_color_hover_3 }};

/* Typography */
$font-family-name_body: {{ settings.fonts_name_1 }};
$font-family-name_tag: {{ settings.fonts_name_2 }};
$font-family-name_nav: {{ settings.fonts_name_3 }};
$font-family-name_alt: {{ settings.fonts_name_4 }};
$font-weight-body: {{ settings.fonts_weight_1 }}; 
$font-weight-tag: {{ settings.fonts_weight_2 }};
$font-weight-nav: {{ settings.fonts_weight_3 }};
$font-weight-alt: {{ settings.fonts_weight_4 }};
$transforms-breadcrumb: {{ settings.enable_transforms }};
$transforms-button: {{ settings.enable_buttons }};
$transforms-title: {{ settings.enable_title }};
$transforms-nav: {{ settings.enable_navigation }};
html,body {
  font-size: $body-font-size + px;
}

body h1,body .h1 {
  font-size: 46px;
}
body h2,body .h2 {
  font-size: 34px;
}
body h3,body .h3 {
  font-size: 30px;
}
body h4,body .h4 {
  font-size: 24px;
}
body h5,body .h5 {
  font-size: 20px;
}
body h6,body .h6 {
  font-size: 16px;
}
body .alt-font {
  font-family: $font-family-name_alt !important;
  font-weight: $font-weight-alt !important;
}

@media (max-width: 767px) { 

  {% for i in (1..100) %}
  .mb-{{i}}{
    margin-bottom:{{i}}px
  }
  .mt-{{i}}{
    margin-top:{{i}}px
  }
  .pt-{{i}}{
    top:{{i}}%
  }
  .pl-{{i}}{
    left:{{i}}%
  }
  {% endfor %}

}

// Medium devices (tablets, 768px and up)
@media (min-width: 768px) { 
  {% for i in (1..100) %}
  .mb-sm-{{i}}{
    margin-bottom:{{i}}px
  }
  .mt-sm-{{i}}{
    margin-top:{{i}}px
  }
  .pt-sm-{{i}}{
    top:{{i}}%
  }
  .pl-sm-{{i}}{
    left:{{i}}%
  }
  .size-{{i}}{
    font-size:{{i}}px;
  }
  {% endfor %}
}

// Large devices (desktops, 992px and up)
@media (min-width: 992px) {
  {% for i in (1..100) %}
  .mb-md-{{i}}{
    margin-bottom:{{i}}px
  }
  .mt-md-{{i}}{
    margin-top:{{i}}px
  }
  .pt-md-{{i}}{
    top:{{i}}%
  }
  .pl-md-{{i}}{
    left:{{i}}%
  }
  {% endfor %}

}

// Extra large devices (large desktops, 1200px and up)
@media (min-width: 1200px) {
  {% for i in (1..100) %}
  .mb-lg-{{i}}{
    margin-bottom:{{i}}px
  }
  .mt-lg-{{i}}{
    margin-top:{{i}}px
  }

  .pt-lg-{{i}}{
    top:{{i}}%
  }
  .pl-lg-{{i}}{
    left:{{i}}%
  }
  {% endfor %}
}

/* Button Color */
.clv-btn-1 {
  color: $primary-bt-text;
  border: 1px solid $primary-bt-border;
  background-color: $primary-bt-bg;
  &:hover {
    color: $primary-bt-text-hover;
    border-color: $primary-bt-border-hover;
    background-color: $primary-bt-bg-hover;
  }
}
.clv-btn-2 {
  color: $secon-bt-text;
  border: 1px solid $secon-bt-border;
  background-color: $secon-bt-bg;
  &:hover {
    color: $secon-bt-text-hover;
    border-color: $secon-bt-border-hover;
    background-color: $secon-bt-bg-hover;
  }
}

.clv-btn-3 {
  color: $secon-bt-text_3;
  border: 1px solid $secon-bt-border_3;
  background-color: $secon-bt-bg_3;
  &:hover {
    color: $secon-bt-text-hover_3;
    border-color: $secon-bt-border-hover_3;
    background-color: $secon-bt-bg-hover_3;
  }
}

body {
  color: $body-text;
  font-style: normal;
  font-weight: $font-weight-body;
  line-height: 1.4;
  font-family: $font-family-name_body;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-rendering: optimizeLegibility;
  font-display: auto;
  a {  
    color: $body-link;
    &:hover {
      color: $body-hover;
    }
  }
  h1,h2,h3,h4,h5,h6 {
    font-family: $font-family-name_tag;
    font-weight: $font-weight-tag;
  }

}
body {
  .menu-top {
    .menu-action {
      font-family: $font-family-name_nav;
      h1,h2,h3,h4,h5,h6 {
        font-family: $font-family-name_nav;
      }
    }
  }
}
body.template-index {
  {% if settings.bg_body_color != '' %}
  background-color: $body-bg;
  {% endif %}
  {% if settings.bg_body_img == 'bg-body.png' %}
  background-image: url({{'bg-body.png' | asset_url }});
  {% elsif settings.bg_body_img != 'none' %}
  background-image: url({{ settings.bg_body_img | asset_url }});
  {% endif %}
  background-position: {{ settings.bg_body_position_x }} {{ settings.bg_body_position_y }};
  background-repeat: {{ settings.bg_body_repeat }};
  background-attachment: {{ settings.bg_body_attachment }};
  background-size: auto !important;
}

/* Header Css */

.promotion-box {
  height: 40px;
  line-height: 40px;z-index:999;
  text-align: center;
  position: relative;
  background: $header-promotion-bg;
  a {
    color: $header-promotion-text;
    font-size: 16px;
    text-transform: unset;
    &:hover {
      text-decoration: underline;
      color: inherit !important;
    }
  }
  .close-promobox {
    position: absolute;
    right: 20px;
    top: 50%;
    transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    z-index: 9;
    cursor: pointer;
    color: {{ settings.promotion_box_color }};
  }
} 

.header {
  background-color: $header-bg;
  a {
    color: $header-link;
    &:hover {
      color: $header-link-hover;
    }
  }
  .header-cart-container.menu {
    text-align: right;
    i,.minicart-wr span,.pre-currencies,.wgcurrent a {
      color: $header-link;
      &:hover {
        color: $header-link-hover;
      }
    }
  }
  .dropdown-sub {
    background-color: $header-bg-dropdown;
    .menu-title {
      a {
        color: $header-dr-title-text;
      }
    }
    li {
      a {
        color: $header-dr-link;
        &:hover {
          color: $header-dr-link-hover;
        }
      }
    }
  }
  &.is-ticky {
    background-color: $header-sticky-bg;
    a {
      color: $header-sticky-link;
      &:hover {
        color: $header-sticky-hover;
      }
    }
    .header-cart-container.menu {
      i,.minicart-wr span,.pre-currencies,.wgcurrent a {
        color: $header-sticky-link;
        &:hover {
          color: $header-sticky-hover;
        }
      }
    }
  }
  .top-bar-dropdown {
    background-color: $header-bg-dropdown;
    a {
      color: $header-dr-link;
      &:hover {
        color: $header-dr-link-hover;
      }
    }
  }
  .header-inner {
    .menu-main-home {
      .site-nav {
        > li {
          > a {
            font-size: 15px;
            line-height: 50px;
            letter-spacing: 2px;
            padding-bottom: 0;
            display: flex;
            align-items: center;
            font-weight: $font-weight-nav;
            text-transform: $transforms-nav;
          }
          &.level-top:hover {
            > a {
              color: $body-hover;
            }
          }
        }
      }
    }
  }
}

.cart-count {
  color: $header-text-count;
  background-color: $header-bg-count;
  pointer-events: none;
  &.empty-cart {
    display: none;
  }
}
.header-topbar {
  background-color: $header-top-bg;
  color: $header-top-text;
  font-size: 14px;
  .header-cart-container {
    .header-currency {
      line-height: 1;
      #currencies-top {
        display: inline-block;
        padding: 0;
        label {
          display: none;
        }
        i {
          font-size: 10px;
        }
        .dropdown-toggle {
          font-size: 13px;
          color: $header-top-text;
        }
        .language-currency {
          .title-selector {
            font-size: 14px;
          }
        }
      }
    }
    .language-currency  {
      padding-left: 10px;
      position: relative;
      .title-selectors {
        display: none;
      }
    }
    .top-currency {
      .cusstom-link {
        li {
          width: auto;
          padding: 0 10px;
          border: 0;
          a {
            color: $header-top-link;
            &:hover {
              color: $header-top-link-hover;
            }
          }
        }
      }
    }
  }
  a {
    color: $header-top-link;
    &:hover {
      color: $header-top-link-hover;
    }
  }
  #currencies-top {
    label {
      display: none;
    }
    .pre-currencies {
      img {
        margin-right: 5px;
        max-width: 17px;
        position: relative;
        top: -1px;
      }
    }
    .dropdown-currency {
      ul {
        li {
          a {
            color: #000;
            img {
              width: 17px;
              margin-right: 8px;
            }
          }
        }
      }
    }
  }
  .top-currency {
    .top-bar-dropdown {
      position: unset;
      width: auto;
      padding: 0;
      opacity: 1;
      visibility: visible;
      background: transparent;
      .cusstom-link {
        .customer-links {
          li {
            i {
              color: $header-top-link;
              margin-right: 5px;
            }
          }
        }
      }
    }
  }
}

.header {
  .header-cart-container {
    .header-cart {
      .top-cart-wr {
        .top-cart {
          .minicart-wr {
            text-align: left;
            padding-left: 8px;
            span {
              display: block;
            }
            .minicart-title {
              padding-right: 0px;
              text-transform: capitalize;
              font-size: 13px;
              font-weight: 400;
            }
            .price-cart-mini {
              font-size: 16px;
              font-weight: 400;
              padding:0;
            }
          }
        }
      }
    }
  }
  .menu-text-right {
    a {
      font-size: 14px;
      line-height: 50px;
      text-transform: uppercase;
      font-weight: 700;
      letter-spacing: 1px;
      text-align: center;
      display: inline-block;
      width: 100%;
      &:hover {
        background: $body-hover;
        color: #fff;
      }
    }
  }
  .search-desktop {
    #search-top {
      input[type=text] {
        border-radius: 0;
        padding: 0 50px 0 10px;
        font-size: 14px;
        height: 45px;
        color: #222;
        background: #f6f6f6;
        border: 0;
        width: 100%;
        border: 0;
        &:focus {
          border: 0 !important;
        }
      }
      button {
        position: absolute;
        right: 0;
        top: 0;
        width: 55px;
        height: 45px;
        line-height: 45px;
        color: #bcbbb9;
        font-size: 0;
        border: 0;
        border-radius: 0;
        -webkit-border-radius: 0;
        -moz-border-radius: 0;
        z-index: 99;
        background-color: transparent;
        &:after {
          font-size: 18px;
        }
      }
    }
  }
}

.header-layout-01 {
  #header-content {
    .header {
      .header-inner {
        padding: 20px 0;
        > .row {
          align-items: center;
        }
        .logo-container {
          text-align: center;
        }
      }
    }
    .header-bottom {
      .header-inner {
        padding: 0;
      }
    }
  }
}
.header-layout-02 {
  .header-inner {
    padding: 20px 0;
    > .row {
      align-items: center;
    }
    .logo-container {
      text-align: left;
    }
    .menu-main-home {
      text-align: center;
    }
  }
}

.header-layout-03 {
  #header-content {
    .header {
      .header-inner {
        padding: 20px 0;
        > .row {
          align-items: center;
        }
        .menu-main-home {
          margin-top: 10px;
          .site-nav>li {
            &:first-child {
              >a {
                padding-left: 0;
              }
            }
          }
        }
        .custom_content-text {
          display: flex;
          justify-content: space-between;
          .e-support {
            i {
              color: #999;
              float: left;
              font-size: 30px;
              height: 45px;
              line-height: 45px;
              width: 45px;
              position: relative;
              top: -5px;
              margin-right: 10px;
            }
            h3 {
              font-family: Poppins;
              font-size: 14px;
              font-weight: 500;
              text-transform: uppercase;
              color: #333333;
              margin: 0;
            }
            p {
              font-size: 12px;
              color: #666666;
              text-transform: capitalize;
              margin: 0;
            }
          }
        }
        .custom-button {
          text-align: right;
        }
      }
    }
  }
}

.header-layout-04 {
  #header-content {
    .header {
      .header-inner {
        padding: 20px 0;
        > .row {
          align-items: center;
        }
        .customer-links {
          li {
            border: 0;
          }
        }
        .menu-main-home {
          margin-top: 30px;
        }
        .header-cart-container {
          text-align: left;
          margin-top: 30px;
          .block-bottom {
            display: block !important;
            width: 100%;
            margin-top: 30px;
            .footer-icon-share {
              i {
                font-size: 16px;
              }
            }
          }
        }
      }
    }
  }
}

.header-layout-05 {
  #header-content {
    position: absolute;
    width: 100%;
    top: 30px;
    z-index: 99999;
    padding-top: 0;
    background: transparent;
    transition: all 0.5s;
    -webkit-transition: 0.5s;
    .header {
      background-color: transparent;
      .header-inner {
        background: #fff;
        padding: 0 15px;
        > .row {
          align-items: center;
          height: 80px;
        }
      }
    }
  }
}

/* Label Product */
.product-label {
  background: $label-sale-bg;
  border: 1px solid $label-sale-border;
  color: $label-sale-text;
}
.label-new {
  background: $label-new-bg;
  border: 1px solid $label-new-border;
  color: $label-new-text;
}

.price-box {
  color: $price;
  .old-price {
    color: $price-compare;
  }
}
.breadcrumb-wrap {
  background-color: $breadcrumb-bg;
  position: relative;
  text-align:{{ settings.breadcrumb_align }};
  border-top: 1px solid $breadcrumb-border;
  margin-bottom: 60px;
  text-transform: $transforms-breadcrumb;
  .breadcrumb_position {
    padding: {{ settings.padding_background_breadcrumb }}px 0;
    strong {
      color: #959595;
      font-weight: 400;
    }
  }
}
.template-product {
  .label-heading {
    display: inline-block;
    float: left;
    width: 100%;
    font-size: 16px;
    margin: 0;
    border: none;
    padding: 0 0 15px;
    font-weight: 700;
    color: #000;
    .value-variant {
      color: #959595;
      font-weight: 400;
      padding-left: 3px;
      &:after {
        content: "*";
        padding-left: 3px;
        color: #ff0000;
      }
    }
  }
}


{% for i in (1..5) %}
