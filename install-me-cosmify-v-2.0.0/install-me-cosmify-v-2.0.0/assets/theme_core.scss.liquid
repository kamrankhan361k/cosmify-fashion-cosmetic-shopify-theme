.tabs-product-home {
  .heading-section-tabs {
    padding-bottom: 30px;
    display: flex;
    justify-content: center;
    align-items: center;
    .heading-block {
      h3 {
        text-align: left;
        font-size: 36px;
        line-height: 1;
        font-weight: 600;
        color: #000;
        position: relative;
      }
    }
    .heading-tabs {
      padding-bottom: 0;
      border: 0;
      text-align: right;
      margin-bottom: 0;
    }
  }
}
.home-product-list {
  .home-product-list-items {
    &+.home-product-list-items {
      border-top: 1px solid #ececec;
      padding-top: 10px;
    }
    .grid-item {
      .product-inner {
        .form-add-cart {
          display: flex;
          .product-image {
            width: 120px;
            .product-label,.label-new,.add-to-link,.add-to-cartS {
              display: none !important;
            }
          }
          .product-content {
            width: calc(100% - 75px);
            text-align: left;
            padding-left: 15px;
            .spr-badge {
              text-align: left;
            }
            .product-collection__options {
              display: none;
            }
            h3 {
              margin-top: 0;
            }
          }
        }
        .wishlist-wr,.compare-wr {display: none;}
      }
    }
  }
}
.home-navigation {
  padding-right: 0;
  .parent-title {
    background-color: $body-hover;
    height: 60px;
    line-height: 60px;
    color: #ffffff;
    font-size: 15px;
    text-transform: uppercase;
    font-weight: bold;
    cursor: pointer;
    letter-spacing: 1.5px;
    padding-left: 20px;
    i {
      font-size: 14px;
      line-height: 60px;
      float: left;
      padding-right: 17px;
    }
  }
  #verticalmenu {
    background: #fff;
    position: unset;
    transform-origin: 0 0 0;
    display: block;
    .nav-verticalmenu {
      width: 100%;
      background: #fff;
      border: 1px solid #ebebeb;
      border-top: 0;
      border-bottom: 0;
      > li {
        border-bottom: 1px solid #ebebeb;
        margin: 0;
        float: none;
        display: block;
        position: relative;
        cursor: pointer;
        height: 48px;
        line-height: 48px;
        > a {
          padding: 0 20px;
          display: inline-block;
          .menu-titlea {
            font-size: 15px;
            text-transform: capitalize;
            margin-bottom: 0;
            line-height: 46px;
          }
          i {
            float: right;
            line-height: 50px;
            padding-right: 10px;
          }
        }
        .dropdown-menu {
          backface-visibility: hidden;
          margin-left: 0px;
          padding-left: 15px;
          margin-top: 0px;
          box-shadow: none;
          opacity: 0;
          top: 0px;
          min-width: 230px;
          transform-origin: 0px 100% 0px;
          visibility: hidden;
          z-index: 999;
          min-height: 482px;
          display: block;
          left: 100% !important;
          right: auto !important;
          list-style: none outside none;
          border-radius: 0px;
          border-width: 1px;
          border-style: solid;
          border-color: rgb(235, 235, 235);
          border-image: initial;
          position: absolute;
          .dropdown-menu-inner {
            line-height: 50px;
            padding: 0px;
            overflow: hidden;
            .mega-col-inner {
              ul {
                float: none;
                border-width: 0px;
                border-style: initial;
                border-color: initial;
                border-image: initial;
                list-style: none;
                li {
                  float: none;
                  height: 45px;
                  line-height: 45px;
                  border-bottom: 1px solid rgb(235, 235, 235);
                }
              }
            }
          }
        }
        &:hover {
          .dropdown-menu {
            opacity: 1;
            visibility: visible;
          }
        }
      }
    }
  }
}
