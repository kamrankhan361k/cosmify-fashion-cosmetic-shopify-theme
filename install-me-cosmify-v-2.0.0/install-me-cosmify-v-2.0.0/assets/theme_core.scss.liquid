{% capture lablenew_color %}menuitem_title_label_color_{{ i }}{% endcapture %}
{% capture lablenew_bg %}menuitem_title_label_background_{{ i }}{% endcapture %}
{% assign lablenewcolor = settings[lablenew_color]  %}
{% assign lablenewbg = settings[lablenew_bg]  %}
.lable-new-{{ intMega }}{
  color:{{lablenewcolor}};
  background:{{lablenewbg}};
}
{%endfor %}

.search-mobile-dark {
  .search-top-container {
    #search-top {
      .search-form {
        .searchbox {
          &.s-loading:before {
            background: url(s-loading.gif);
            background-repeat: no-repeat;
            background-position: 100%;
            background-size: 30px;
            content: "";
            width: 30px;
            height: 30px;
            position: absolute;
            z-index: 104;
            right: 140px;
            top: 9px;
          }
          .search-bar {
            display: block;
            .input-group-field {
              border-style: solid;
              border-width: 1px;
              border-color: #e7e7e7;
              border-radius: 36px;
              height: 36px;
              width: 100%;
              &:focus {
                border: 1px solid #e7e7e7 !important;
              }
            }
            .input-group-btn {
              button {
                position: absolute;
                right: 0;
                top: 0;
                height: 50px;
                line-height: 50px;
                font-size: 15px;
                z-index: 99;text-align:center;
                border: 0;
                border-radius: 0 5px 5px 0;
                width: 120px;
                padding: 0 20px;
                letter-spacing: 1.6px;
                font-weight: 700;
                @extend .clv-btn-2;
                &:after {
                  display: none;
                }
              }
            }
            .search-results {
              position: absolute !important;
              top: 45px !important;
            }
          }
        }
      }
    }
  }
}
#header-content {
  .menu-text-right {
    a {
      background: $body-hover;
      color: #fff;
      border-radius: 4px;
    }
  }
}
.header-absolute {
  .header {
    background-color: transparent;
    box-shadow: none;
    &.is-ticky {
      background-color: $header-sticky-bg;
    }
  }
}
{% capture images-breakcrum %}images-bg-breadcrum.jpg{% endcapture %}
{% if settings.display_backgound_images %}
.breadcrumb-wrap {
  background-image:url({{ images-breakcrum | asset_url }});
  -webkit-background-size: auto;
  -moz-background-size: auto;
  -o-background-size: auto;
  background-size: auto;
  background-position: top center;
  background-repeat: no-repeat;
  background-attachment: fixed;
}
.breadcrumb-wrap .container {padding: 0;}
{% endif %}

/* General Css */
.heading-block {
  text-align: center;
  margin-bottom: 35px;
  h3 {
    clear: both;
    margin-bottom: 0;
    font-size: 36px;
    line-height: 36px;
    font-weight: 600;
    color: #000;
    position: relative;
    text-transform: $transforms-title;
    @media (max-width: 1024px) {
      font-size: 24px;
      line-height: 30px;
    }
  }
  p.subtitle {
    font-size: 16px;
    text-transform: unset;
    color: #7d7d7d;
    line-height: 30px;
    margin-top: 7px;
    margin-bottom: 0;
    font-weight: 400;
  }
}
.product-content {
  h3 {
    margin-top: 10px;
    margin-bottom: 10px;
    line-height: 15px;
    a {
      font-size: 16px;
      font-weight: 600;
      text-transform: capitalize;
      line-height: 20px;
      color: #000;
      &:hover {
        color: $body-hover;
      }
    }
  }
  .price-box {
    font-size: 14px;
    font-weight: 400;
    line-height: 24px;
    transition: all 0.5s;
    margin-top: 0;
    p.regular-product {
      margin-bottom: 0;
    }
    p {
      margin-bottom: 0;
    }
  }
  .spr-badge {
    text-align: center;
    margin-bottom: 10px;
    .spr-starrating {
      margin: 0;
    }
    .spr-badge-caption {
      display: none;
    }
  }
}
.owl-theme {
  .owl-nav {
    position: absolute;
    top: -65px;
    transform: translate(0, 0);
    -ms-transform: translate(0, 0);
    -webkit-transform: translate(0, 0);
    width: 100%;
    left: auto;
    margin-top: 0;
    right: 15px;
    text-align: right;
    i {
      font-size: 14px;
    }
    button {
      opacity: 1;
      -moz-transition: all .4s ease;
      -webkit-transition: all .4s ease;
      -o-transition: all .4s ease;
      transition: all .4s ease;
      -moz-backface-visibility: hidden;
      -webkit-backface-visibility: hidden;
      backface-visibility: hidden;
      &.owl-prev {
        left: calc(100% - 30px);
      }
      &.owl-next {
        right: auto;
      }
    }
  }
  .owl-dots {
    margin-top: 10px;
    .owl-dot {
      height: 15px;
      width: 15px;
      border: 2px solid $body-hover;
      display: inline-block;
      margin: 0 3px;
      border-radius: 50%;
      -moz-transition: all .4s;
      -webkit-transition: all .4s;
      -o-transition: all .4s;
      transition: all .4s;
      -moz-backface-visibility: hidden;
      -webkit-backface-visibility: hidden;
      backface-visibility: hidden;
      span {
        display: none;
      }
      &.active {
        width: 15px;
        border-radius: 15px;
        background: $body-hover;
      }
    }
  }
}

/* Footer General */

.footer-container {
  .footer-center {
    background-color: $footer-bg;
    padding-top: 60px;
    position: relative;
    z-index: 1;
    line-height: 24px;
    h3.module-title {
      letter-spacing: 0.25px;
      font-size: 16px;
      margin: 0;
      padding-bottom: 22px;
      position: relative;
      text-transform: uppercase;
      line-height: 24px;
      font-weight: 700;
    }
    .footer-link {
      ul {
        margin-bottom: 0;
        li {
          margin-bottom: 10px;
          a {
            font-size: 14px;
            line-height: 20px;
            text-transform: capitalize;
          }
        }
      }
    }
    .footer-newsletter {
      .newsletter {
        .title_newsletter {
          font-size: 15px;
          line-height: 30px;
          margin-bottom: 12px;
        }
        .input-group-field {
          line-height: 50px;
          color: #111;
          outline: none;
          padding: 0 10px;
          width: 100%;
          height: 50px;
          border-radius: 0;
          border: 1px solid #ebebeb;
          font-size: 14px;
          background: #fff;
        }
        .input-group-btn {
          width: 100%;
          position: unset;
          margin-left: 0;
          margin-top: 15px;
          > .btn {
            height: 50px;
            line-height: 50px;
            border: 0;
            padding: 0 15px;
            font-size: 16px;
            letter-spacing: 0;
            font-weight: 600;
            width: 100%;
          }
        }
      }
    }
    .custom-html {
      @media (max-width: 767px) {
        text-align: center;
      }
      p {
        font-size: 14px;
        line-height: 20px;
        font-weight: 400;
        color: $footer-text;
      }
    }
    .footer-copyrights {
      padding: 20px 0px 20px 0px;
    }
    .footer-isocal {
      padding-top: 30px;
      padding-bottom: 25px;
      .footer-icon-share {
        a {
          display: inline-block;
          font-size: 16px;
          line-height: 33px;
          i {
            text-align: center;
            font-size: 24px;
            margin-right: 5px;
            display: inline-block;
            background: transparent;
            width: auto;
            height: auto;
            line-height: 30px;
            border-radius: 100%;
            padding: 0 10px;
          }
          &:hover {
            i {
              color: $body-hover;
            }
          }
          &:first-child {
            margin-left: 0;
          }
        }
      }
    }
  }
  .footer-contact {
    p {
      margin-top: 20px;
      line-height: 30px;
      font-size: 16px;
      &:before {
        content: '';
        font-family: 'cleversoft' !important;
        speak: none;
        font-style: normal;
        font-weight: 400;
        font-variant: normal;
        text-transform: none;
        letter-spacing: 0;
        -webkit-font-feature-settings: "liga";
        -moz-font-feature-settings: "liga=1";
        -moz-font-feature-settings: "liga";
        -ms-font-feature-settings: "liga" 1;
        font-feature-settings: "liga";
        -webkit-font-variant-ligatures: discretionary-ligatures;
        font-variant-ligatures: discretionary-ligatures;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
        font-size: 30px;
        padding-right: 20px;
        line-height: 60px;
        float: left;
        height: 60px;
        display: inline-block;
      }
      &.clever-address:before {
        content: "\c102";
      }
      &.clever-email:before {
        content: "\c048";
      }
      &.clever-phone:before {
        content: "\c032";
      }
    }
  }
  .footer-instagram {
    a {
      width: 25%;
      float: left;
      padding: 2px;
    }
  }
  .footer-bottom {
    margin-top: 30px;
    .footer-isocal {
      @media (max-width: 767px) {
        text-align: center;
      }
      .footer-icon-share {
        a {
          display: inline-block;
          clear: both;
          font-size: 14px;
          margin-bottom: 10px;
          text-transform: capitalize;
          i {
            text-align: center;
            font-size: 14px;
            margin-right: 5px;
            display: inline-block;
            background: #f3f3f3;
            width: 40px;
            height: 40px;
            line-height: 40px;
            border-radius: 100%;
          }
          &:hover {
            i {
              color: #fff;
              background: $body-hover;
            }
          }
          &:first-child {
            margin-left: 0;
          }
        }
      }
    }
    .footer-copyright {
      @media (max-width: 767px) {
        text-align: center;
      }
      address {
        font-size: 15px;
        line-height: 20px;
        margin-top: 16px;
        color: $footer-text;
      }
    }
  }
  &.footer_layout_1 {
    .footer-center {
      .custom-html {
        text-align: right;
        @media (max-width: 767px) {
          text-align: center;
        }
      }
      .footer-copyright,.custom-html {
        margin-top: 40px;
      }
      .footer-copyright {
        text-align: left;
        @media (max-width: 767px) {
          text-align: center;
        }
      }
      .footer-isocal {
        text-align: right;
        @media (max-width: 767px) {
          text-align: center;
        }
        .footer-icon-share {
          margin-top: 20px;
          a {
            display: inline-block;
            i {
              margin-right: 10px;
              border-radius: 100%;
              border: 2px solid;
              font-size: 14px;
              transition: all 0.25s ease 0s;
              -webkit-transition: all 0.25s ease 0s;
              -moz-transition: all 0.25s ease 0s;
              -o-transition: all 0.25s ease 0s;
              -ms-transition: all 0.25s ease 0s;
              padding: 0;
              width: 36px;
              height: 36px;
              text-align: center;
              padding: 0;
              line-height: 32px;
            }
          }
        }
      }
      .footer-logo {
        margin-bottom: 25px;
      }
    }
  }
  &.footer_layout_2 {
    .footer-center {
      .footer-link {
        @media (max-width: 767px) {
          text-align: center;
        }
        ul {
          li {
            display: inline-block;
            padding: 0 5px;
            margin: 0;
            a {
              font-size: 14px;
              line-height: 30px;
              font-weight: 400;
            }
          }
        }
        &.footer_layout_2 {
          display: inline-block;
        }
      }
      .footer-copyright {
        address {
          margin-top: 0;
          margin-bottom: 0;
          line-height: 30px;
          @media (max-width: 767px) {
            text-align: center;
          }
        }
      }
      .col-md-6.col-12.text-right {
        @media (max-width: 767px) {
          text-align: center;
        }
        .footer-isocal {
          padding-top: 0;
          padding-bottom: 0;
          display: inline-block;
          padding-left: 20px;
          .footer-icon-share {
            a {
              display: inline-block;
              margin-bottom: 0;
              font-size: 12px;
              line-height: 20px;
              padding: 0;
              i {
                font-size: 18px;
                border-radius: 0;
                padding: 0 5px;
              }
            }
          }
        }
      }
    }
  }
  &.footer_layout_3 {
    .footer-center {
      .footer-isocal {
        .footer-icon-share {
          a {
            display: block;
            margin-bottom: 10px;
            i {
              line-height: 30px;
              width: 30px;
              height: 30px;
              border-radius: 15px;
              border: 1px solid #ffffff;
              text-align: center;
              font-size: 14px;
              margin-right: 10px;
            }
          }
        }
      }
      .footer-customhtml {
        p {
          font-size: 14px;
          line-height: 33px;
          letter-spacing: 0.5px;
          margin-bottom: 15px;
          margin-top: 15px;
        }
      }
      .footer-copyright {
        margin-top: 30px;
      }
    }
  }
  &.footer_layout_4 {
    .footer-newsletter {
      margin-bottom: 50px;
      .newsletter {
        display: flex;
        align-items: center;
        justify-content: space-between;
        h3 {
          letter-spacing: 1px;
          padding-right: 10px;
          font-size: 20px;
          font-weight: 400;
          padding-bottom: 0;
        }
      }
    }
    .footer-customhtml {
      margin-bottom: 50px;
      font-size: 26px;
      font-weight: 400;
      text-align: right;
    }
    .footer-isocal {
      .footer-icon-share {
        a {
          display: inline-block;
          margin-right: 10px;
          i {
            width: 35px;
            height: 35px;
            background: #fff;
            color: #4e4d4d;
            font-size: 20px;
            line-height: 35px;
            text-align: center;
            margin-right: 0;
          }
        }
      }
    }
    .footer-copyright {
      margin-top: 50px;
    }
    .footer-logo {
      margin-top: 20px;
    }
  }
}

