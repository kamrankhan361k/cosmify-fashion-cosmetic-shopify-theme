  <head>

    <!-- Basic page needs ================================================== -->
    <meta charset="utf-8">

    <!-- Title and description ================================================== -->
    <title>
      {{ page_title }}{% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}{% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
    </title>

    {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}">
    {% endif %}


    <!-- Helpers ================================================== -->
    <link rel="canonical" href="{{ canonical_url }}">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    {% include 'meta-social' %}
    <!-- Favicon -->
    {% if settings.favicon != blank %}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
    {% endif %}
    {% include 'font-family' %}


    {{ 'lazysizes.js' | asset_url | script_tag }}
    <!-- Styles -->
    {% if settings.body_rtl %}
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.rtlcss.com/bootstrap/v4.2.1/js/bootstrap.min.js" integrity="sha384-a9xOd0rz8w0J8zqj1qJic7GPFfyMfoiuDjC9rqXlVOcGO/dmRqzMn34gZYDTel8k" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdn.rtlcss.com/bootstrap/v4.2.1/css/bootstrap.min.css" integrity="sha384-vus3nQHTD+5mpDiZ4rkEPlnkcyTP+49BhJ4wJeJunw06ZAp+wzzeBPUXr42fi8If" crossorigin="anonymous">
    {{ 'styles-rtl.css' | asset_url | stylesheet_tag }}
    {% else %}
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    {% endif %}
    {% if settings.layout_fullpage %}
    {{ 'fullpage.css' | asset_url | stylesheet_tag }}
    {% endif %}
    <!-- Theme base and media queries -->
    {{ 'theme_exention.css' | asset_url | stylesheet_tag }}
    {{ 'styles.css' | asset_url | stylesheet_tag }}
    {{ 'owl.theme.css' | asset_url | stylesheet_tag }}
    {{ 'theme_core.scss.css' | asset_url | stylesheet_tag }}
    {{ 'responsive.css' | asset_url | stylesheet_tag }}
    {% if settings.display_csseditor %}
    {{ 'bannereditor.css' | asset_url | stylesheet_tag }}
    {% endif %}
    <!-- Scripts -->
    {% include 'head_scripts' %}
    {{ 'skrollr.js' | asset_url | script_tag }}
    {{ "tricky3.infinite-scroll.v2.min.js" | asset_url | script_tag }}
    <!-- 360 Product -->
    {{ 'threesixty.min.js' | asset_url | script_tag }}
    <!-- Header hook for plugins ================================================== -->
    {{ content_for_header }}
    {%- capture CFH -%}{{ content_for_header }}{%- endcapture -%}
    {%- if CFH contains 'design_mode' -%}
    {%- assign isAdmin = true -%}
    {%- else -%}
    {%- assign isAdmin = false -%}
    {%- endif -%}
    <!--[if lt IE 9]>
{{ '//html5shiv.googlecode.com/svn/trunk/html5.js' | script_tag }}
<![endif]-->

    {% comment %}
    If you store has customers disabled, you can remove the following JS file
    {% endcomment %}
    {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
    {% endif %}

    {% include 'weglot_hreftags' %}
    {% include 'weglot_switcher' %}
  </head>

  {% comment %}
  Add the page template as a class for easy page or template specific styling.
  {% endcomment %}
  <body id="{{ page_title | handle }}" class="{% if settings.layout_width_body %}boxed-layout {% endif %}{{ settings.layout_fullpage_ancho}}{% if customer %} customer-logged-in{% endif %}{% if settings.body_rtl %} rtl{% else %} ltr{% endif %} template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}">
    <div id="loading"></div>
    <div class="wrapper-container">
      <div class="header-wrapper">
        {% section 'header' %}
      </div>
      <div class="product-quickview"></div>
      {% include 'breadcrumb' %}
      <main class="main-content" >
        {% if settings.header_layout == "header-vertical-left" %}
        {% include 'header-search-cart' %}
        {% endif %}

        <div class="{% unless template contains 'collection' %}container {% endunless %}main_container">
          {{ content_for_layout }}
        </div>
      </main>
      {% section 'footer' %}
      {% section 'popups' %}
      {% include 'message-modal' %}
      {% include 'quick-view-product' %}
      {% include 'popup-newsletter' %}
      {% include 'layout-compare' %}

    </div>
    {% include 'canvas-menu' %}
    {% include 'search-autocomplete' %}
    {% include 'footer_scripts' %}
    {% if template contains 'index' and isAdmin %}
    {% include 'setup_demo' %}
    {% endif %}
    {{ weglot_includes }}
    <style>
      {% if settings.use_scale_product_image %}
      .products-grid .grid-item .grid-image img{width:auto;max-height:{{settings.product_scale_height }}px;}
      .products-grid .grid-item .product-image{height:{{settings.product_container_scale_height}}px;}
      {% endif %}
    </style>
    {% if settings.age_verify %}
    <div class="age-verify" style="">
      <div class="content-popup">
        {% if settings.age_verify_title != blank %}
        <h3> {{settings.age_verify_title }}</h3>
        {% endif %}
        {% if settings.age_verify_subtitle != blank %}
        <p>{{settings.age_verify_subtitle }}</p>
        {% endif %}
        <div class="button">
          <a class="enter-page">Enter <i class="cs-font clever-icon-arrow-right-4"></i></a> or <a class="button-close" href="{{settings.age_verify_link}}">Exit</a>
        </div>
      </div>
    </div>
    <style>
      .age-verify{display:none;}
    </style>
    <script>
      $(document).ready(function(){
        $(window).load(function() {
          // if no cookie
          // if no cookie
          if ($.cookie('alert') != "true") {
            $('.age-verify').show();
            $(".age-verify .enter-page").click(function () {
              $(".age-verify").slideUp("slow");
              // set the cookie for 24 hours
              var date = new Date();
              date.setTime(date.getTime() + 24 * 60 * 60 * 1000);
              $.cookie('alert', "true", {
                expires: date
              });
            });
          }

        });
      });
    </script>
    {% endif %}




